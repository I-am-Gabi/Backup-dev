<project default="formatTest">

   <!-- = = = = = = -->
   <!-- formatTest  -->
   <!-- = = = = = = -->
   <target name="formatTest" depends="init" description="--> test target one">
      <!-- hello world -->
      <greeting />
   </target>

   <!-- - - - - - - -->
   <!-- init        -->
